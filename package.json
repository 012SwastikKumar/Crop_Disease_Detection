{
  "name": "crop_disease_detection",
  "version": "1.0.0",
  "main": "index.html",
  "scripts": {
    "sass": "node-sass public/sass/main.scss public/css/style.css -w",
    "compile:scss": "node-sass public/sass/main.scss public/css/style.css",
    "add-prefix": "postcss --use autoprefixer -b 'last 10 versions' public/css/style.css -o public/css/styleWithPrifix.css",
    "compress:css": "node-sass public/css/styleWithPrifix.css public/css/main.css --output-style compressed",
    "build:css": "npm-run-all compile:scss add-prefix compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/012SwastikKumar/Crop_Disease_Detection.git"
  },
  "author": "Swastik Kumar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/012SwastikKumar/Crop_Disease_Detection/issues"
  },
  "homepage": "https://github.com/012SwastikKumar/Crop_Disease_Detection#readme",
  "description": "detect the diseases from given crops.",
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "concat": "^1.0.3",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.7",
    "postcss-cli": "^9.1.0"
  }
}
